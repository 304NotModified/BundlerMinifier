  <KeyBindings>
    <KeyBinding guid="guidBundlerCmdSet" id="UpdateSolution" mod1="Shift Alt" key1="I" editor="guidVSStd97"/>
    <KeyBinding guid="guidBundlerCmdSet" id="CreateBundleId" mod1="Shift Alt" key1="F" editor="guidVSStd97"/>
