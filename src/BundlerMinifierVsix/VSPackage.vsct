        <CommandFlag>DynamicVisibility</CommandFlag>
        <CommandFlag>DefaultInvisible</CommandFlag>
        <Strings>
          <ButtonText>Re-Minify all bundles in solution</ButtonText>
        </Strings>
      </Button>
    </Buttons>
